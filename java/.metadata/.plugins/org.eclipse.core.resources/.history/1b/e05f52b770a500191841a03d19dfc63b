
public class Car {
	
	private int whell= 4;
	private int doors= 4;
	private int seats= 4;
	private int speed;
	public int getWhell() {
		return whell;
	}
	public void setWhell(int whell) {
		this.whell = whell;
	}
	public int getDoors() {
		return doors;
	}
	public void setDoors(int doors) {
		this.doors = doors;
	}
	public int getSeats() {
		return seats;
	}
	public void setSeats(int seats) {
		this.seats = seats;
	}
	public int getSpeed() {
		return speed;
	}
	public void setSpeed(int speed) {
		this.speed = speed;
	}
	
}
