package question1;

public class BMV extends Car {

	private int maxSpeed= 200;
	public void info() {
		System.out.println(getWhell());
		System.out.println(getSeats());
		System.out.println(getDoors());
		System.out.println(this.maxSpeed);
	}
	
	public void Speed() {
		for(int i=0;i<10;i++) {
			System.out.println(this.maxSpeed);
		}
	}
}
