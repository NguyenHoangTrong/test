package question1;

public class Car {

	private int whell= 4;
	private int doors= 4;
	private int seats= 4;
	private int maxSpeed;
	
	public void info() {
		System.out.println("Whell: "+""+getWhell());
		System.out.println("Seats: "+""+getSeats());
		System.out.println("Doors: "+""+getDoors());	
	}
	
	public void run() {
		for(int i=0; i<10; i++) {
			System.out.println(this.maxSpeed);
		}
	}
	
	public int getWhell() {
		return whell;
	}
	public void setWhell(int whell) {
		this.whell = whell;
	}
	public int getDoors() {
		return doors;
	}
	public void setDoors(int doors) {
		this.doors = doors;
	}
	public int getSeats() {
		return seats;
	}
	public void setSeats(int seats) {
		this.seats = seats;
	}
	public int getSpeed() {
		return maxSpeed;
	}
	public void setSpeed(int speed) {
		this.maxSpeed = speed;
	}



	public int getMaxSpeed() {
		return maxSpeed;
	}



	public void setMaxSpeed(int maxSpeed) {
		this.maxSpeed = maxSpeed;
	}
	
	
	
	
}
