package question2;

public class Car implements Runnable{

	private int whell= 4;
	private int doors= 4;
	private int seats= 4;
	private int maxSpeed;
	
	@Override
	public void run() {
		for(int i=0; i<10; i++) {
			System.out.println(maxSpeed);
		}
	}
	
	public void info() {
		System.out.println(this.whell);
		System.out.println(this.doors);
		System.out.println(this.seats);
		System.out.println(maxSpeed);
	}

}
