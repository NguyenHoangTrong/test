package question2;

public class Car implements Runnable{
	
	private int whell= 4;
	private int doors= 4;
	private int seats= 4;
	private int maxSpeed;

	@Override
	public void info() {
		
	}

	@Override
	public void maxSpeed() {
		
	}

	public int getWhell() {
		return whell;
	}

	public void setWhell(int whell) {
		this.whell = whell;
	}

	public int getDoors() {
		return doors;
	}

	public void setDoors(int doors) {
		this.doors = doors;
	}

	public int getSeats() {
		return seats;
	}

	public void setSeats(int seats) {
		this.seats = seats;
	}

	
}
