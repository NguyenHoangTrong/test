package question1;

public class Car {

	private int whell= 4;
	private int doors= 4;
	private int seats= 4;
	private int speed;
	public int getWhell() {
		return whell;
	}
	public void setWhell(int whell) {
		this.whell = whell;
	}
	public int getDoors() {
		return doors;
	}
	public void setDoors(int doors) {
		this.doors = doors;
	}
	public int getSeats() {
		return seats;
	}
	public void setSeats(int seats) {
		this.seats = seats;
	}
	public int getSpeed() {
		return speed;
	}
	public void setSpeed(int speed) {
		this.speed = speed;
	}
	
	public void Speed() {
		for(int i=0;i<10;i++) {
			System.out.println(this.speed);
		}
	}
	
	public void info() {
		System.out.println(this.whell);
		System.out.println(this.seats);
		System.out.println(this.doors);
		System.out.println(this.speed);
	}
}
